		{
			"name": "register",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Check if the response body is not empty",
							"if (pm.response.text() && pm.response.text().trim() !== \"\") {",
							"    var jsonData = pm.response.json();",
							"",
							"    // Check if the access token exists in the response",
							"    if (jsonData && jsonData.access_token) {",
							"        // Set the global variable named \"accessToken\" with the retrieved token",
							"        pm.globals.set(\"accessToken\", jsonData.access_token);",
							"        console.log(\"Access token stored in global variable: \" + jsonData.access_token);",
							"    } else {",
							"        console.log(\"Access token not found in the response.\");",
							"    }",
							"} else {",
							"    console.log(\"Response body is empty. No JSON to parse.\");",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
